mt-tab-container class=\"page-tabbar-container\" v-model=\"active\">\r\n                <!-- 第一个子面板 -->\r\n                <mt-tab-container-item id=\"message\">\r\n                    <messagelist></messagelist>\r\n                </mt-tab-container-item>\r\n            </mt-tab-container>\r\n            <!-- (3)底部导航条tabbar -->\r\n            <mt-tabbar fixed v-model=\"active\">\r\n                                            <!-- native 启用浏览器原生:事件 -->\r\n                <mt-tab-item id=\"message\" @click.native=\"changeState(0)\">\r\n                    <tabbaricon :focused=\"currentIdx[0].isSelect\" :selectedImage=\"require('../../../assets/ic_weixin_selected.png')\" :normalImage=\"require('../../../assets/ic_weixin_normal.png')\"></tabbaricon>\r\n                    微信\r\n                </mt-tab-item>\r\n                <mt-tab-item id=\"contact\" @click.native=\"changeState(1)\">\r\n                    <tabbaricon :focused=\"currentIdx[1].isSelect\" :selectedImage=\"require('../../../assets/ic_contacts_selected.png')\" :normalImage=\"require('../../../assets/ic_contacts_normal.png')\"></tabbaricon>\r\n                    通讯录\r\n                </mt-tab-item>\r\n                <mt-tab-item id=\"find\" @click.native=\"changeState(2)\">\r\n                    <tabbaricon :focused=\"currentIdx[2].isSelect\" :selectedImage=\"require('../../../assets/ic_find_selected.png')\" :normalImage=\"require('../../../assets/ic_find_normal.png')\"></tabbaricon>\r\n                    发现\r\n                </mt-tab-item>\r\n                <mt-tab-item id=\"me\" @click.native=\"changeState(3)\">\r\n                    <tabbaricon :focused=\"currentIdx[3].isSelect\" :selectedImage=\"require('../../../assets/ic_me_selected.png')\" :normalImage=\"require('../../../assets/ic_me_normal.png')\"></tabbaricon>\r\n                    我\r\n                </mt-tab-item>\r\n            </mt-tabbar>\r\n        </div>\r\n        <!-- <h3>Home.vue根组件</h3> -->\r\n    </div>\r\n</template>\r\n<script>\r\n//为按钮绑定点击事件\r\n//修改状态;当前按钮true 其它false\r\n//1.引入顶部导航条子组件\r\nimport TitleBar from \"./common/TitleBar.vue\"\r\n//2.引入消息列表子组件\r\nimport Messagelist from \"./common/Messagelist.vue\"\r\n//3.引入底部导航条图标组件\r\nimport TabBarIcon from \"./common/TabBarIcon.vue\"\r\n\r\nexport default {\r\n//2.注册子组件\r\n    methods:{\r\n        search(){console.log(\"搜索\")},\r\n        add(){console.log(\"添加\")},\r\n        changeState(idx){\r\n            //功能:完成图片状态切换\r\n            //console.log(idx)\r\n            //1.创建循环遍历状态数组\r\n            for(var i=0;i<this.currentIdx.length;i++){\r\n                //2.如果当前下标与idx相同,元素true\r\n                if(i==idx){\r\n                    this.currentIdx[i].isSelect=true;\r\n                }else{//3.其它元素false\r\n                    this.currentIdx[i].isSelect=false;\r\n                }\r\n            }\r\n        }\r\n    },\r\n    components:{\r\n        \"titlebar\":TitleBar, //注册顶部导航条\r\n        \"messagelist\":Messagelist,//注册消息列表\r\n        \"tabbaricon\":TabBarIcon,//注册底部导航条\r\n    },\r\n    data(){\r\n        return{\r\n            active:\"message\",//保存显示哪个子面板id\r\n            currentIdx:[ //下面数组保存按钮状态\r\n                {isSelect:true},//0 第一个按钮状态\r\n                {isSelect:false},//1 第二个按钮状态\r\n                {isSelect:false},//2 第三个按钮状态\r\n                {isSelect:false}//3 第四个按钮状态\r\n            ]\r\n        }\r\n    }\r\n}\r\n//3.在模板中调用\r\n</script>\r\n<style scoped>\r\n    /* 功能:覆盖原生按钮案子颜色:选中 */\r\n    .mint-tabbar>.mint-tab-item.is-selected{\r\n        background-color:transparent;\r\n        color:#45c018;/*绿色 */\r\n    }\r\n    /*默认颜色 */\r\n    .mint-tabbar>.mint-tab-item{\r\n        color:#999999;/*灰色 */\r\n    }\r\n</style>"]}]}