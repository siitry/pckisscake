sole.log(1);\r\n            console.log(uname+\":\"+upwd+\":\"+age);\r\n            //3.使用正则表达式验证用户名\r\n            if(!reg.test(uname)){\r\n                this.$messagebox(\"消息\",\"用户名格式不正确\");\r\n                return;\r\n            }\r\n            //4.如果验证用户名不成功     交互提示框\r\n            //5.如果验证密码不成功      交互提示框\r\n            if(!reg.test(upwd)){\r\n                this.$messagebox(\"消息\",\"密码格式不正确\");\r\n                return;\r\n            }\r\n            //6.如果验证年龄不成功      交互提示框\r\n            if(!regage.test(age)){\r\n                this.$messagebox(\"消息\",\"年龄格式不正确\");\r\n                return;\r\n            }\r\n            //7.发送ajax 请求完成注册任务\r\n            console.log(\"验证结束\");\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src\\components\\web1908\\exam"}]}